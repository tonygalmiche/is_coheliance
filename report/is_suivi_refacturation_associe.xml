<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_suivi_refacturation_associe_tree_view" model="ir.ui.view">        <field name="name">is_suivi_refacturation_associe_tree_view</field>        <field name="model">is.suivi.refacturation.associe</field>        <field name="arch" type="xml">            <tree create="false" colors="orange:type_donnee=='frais'">                <field name="date"/>                <field name="montant"      sum="Total"/>                <field name="type_donnee" />                <field name="affaire_id" widget="many2one_clickable" />                <field name="associe_id" />                <field name="type_frais" />                <field name="type_frais_km" />                <field name="commentaire"/>           </tree>        </field>    </record>    <record id="is_suivi_refacturation_associe_search_view" model="ir.ui.view">        <field name="name">is_suivi_refacturation_associe_search_view</field>        <field name="model">is.suivi.refacturation.associe</field>        <field name="arch" type="xml">            <search>                <filter string="Mes données"   domain="[('associe_id','=',uid)]"                     name="mes_donnees_filter"/>                <filter string="Hors Frais Km" domain="[('type_frais','not like','FRAIS DEPLACEMENT KM')]" name="hors_frais_km_filter"/>                <field name="type_donnee" />                <field name="affaire_id" />                <field name="associe_id" />                <field name="type_frais" />                <group string="Regroupement par">                    <filter string="Type de donnée" context="{'group_by': 'type_donnee'}"/>                    <filter string="Affaire"        context="{'group_by': 'affaire_id'}"/>                    <filter string="Associé"        context="{'group_by': 'associe_id'}"/>                    <filter string="Mois"           context="{'group_by': 'date:month'}"/>                </group>            </search>        </field>    </record>    <record id="is_suivi_refacturation_associe_graph" model="ir.ui.view">         <field name="name">is_suivi_refacturation_associe_graph</field>         <field name="model">is.suivi.refacturation.associe</field>         <field name="arch" type="xml">             <graph type="pivot">                 <field name="date"          type="row"  interval="year"  string="Année"/>                 <field name="date"          type="row"  interval="month" string="Mois" />                 <field name="associe_id"    type="col"/>                 <field name="type_donnee"   type="col"/>                 <field name="type_frais_km" type="col"/>                 <field name="montant"       type="measure"/>             </graph>         </field>    </record>    <record id="is_suivi_refacturation_associe_action" model="ir.actions.act_window">        <field name="name">Suivi refacturation associé</field>        <field name="res_model">is.suivi.refacturation.associe</field>        <field name="view_type">form</field>        <field name="view_mode">graph,tree</field>        <field name="view_id" ref="is_suivi_refacturation_associe_graph"/>        <field name="context">{                'search_default_mes_donnees_filter': 1            }        </field>    </record>    <menuitem name="Analyses associés" id="is_crm_analyses_associes_menu" parent="is_crm_top_menu" sequence="40" />    <menuitem         name="Suivi refacturation associé"         id="is_suivi_refacturation_associe_menu"         parent="is_crm_analyses_associes_menu"         sequence="10"          action="is_suivi_refacturation_associe_action"       />    <record id="is_suivi_refacturation_total_frais_graph" model="ir.ui.view">         <field name="name">is_suivi_refacturation_total_frais_graph</field>         <field name="model">is.suivi.refacturation.associe</field>         <field name="arch" type="xml">             <graph type="pivot">                 <field name="date"       type="row"  interval="year"  string="Année"/>                 <field name="date"       type="row"  interval="month" string="Mois" />                 <field name="type_frais" type="col"/>                 <field name="montant"    type="measure"/>             </graph>         </field>    </record>    <record id="is_suivi_refacturation_total_frais_action" model="ir.actions.act_window">        <field name="name">Refacturation total des frais</field>        <field name="res_model">is.suivi.refacturation.associe</field>        <field name="view_type">form</field>        <field name="view_mode">graph,tree</field>        <field name="view_id" ref="is_suivi_refacturation_total_frais_graph"/>        <field name="domain">[('type_donnee', '=', 'frais')]</field>        <field name="context">{                'search_default_hors_frais_km_filter': 1            }        </field>    </record>    <menuitem name="Analyses administratif" id="is_crm_analyses_administratif_menu" parent="is_crm_top_menu" sequence="50" />    <menuitem         name="Refacturation total des frais"         id="is_suivi_refacturation_total_frais_menu"         parent="is_crm_analyses_administratif_menu"         sequence="10"          action="is_suivi_refacturation_total_frais_action"    /></data></openerp>