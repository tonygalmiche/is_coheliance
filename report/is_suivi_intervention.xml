<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_suivi_intervention_tree_view" model="ir.ui.view">        <field name="name">is_suivi_intervention_tree_view</field>        <field name="model">is.suivi.intervention</field>        <field name="arch" type="xml">            <tree>                <field name="affaire_id"          widget="many2one_clickable" />                <field name="client_id"           widget="many2one_clickable" />                <field name="intitule"/>                <field name="article_id"          widget="many2one_clickable" />                <field name="date"/>                <field name="associe_id"          widget="many2one_clickable" />                <field name="sous_traitant_id"    widget="many2one_clickable" />                <field name="temps_passe"/>                <field name="unite_temps"/>                <field name="montant_facture" sum="Total"/>                <field name="commentaire"/>           </tree>        </field>    </record>    <record id="is_suivi_intervention_form_view" model="ir.ui.view">        <field name="name">is_suivi_intervention_form_view</field>        <field name="model">is.suivi.intervention</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="affaire_id"/>                            <field name="client_id"/>                            <field name="intitule"/>                            <field name="article_id"/>                            <field name="date"/>                            <field name="associe_id"/>                            <field name="sous_traitant_id"/>                            <field name="temps_passe"/>                            <field name="unite_temps"/>                            <field name="montant_facture"/>                            <field name="commentaire"/>                        </group>                    </group>                </sheet>           </form>        </field>    </record>    <record id="is_suivi_intervention_search_view" model="ir.ui.view">        <field name="name">is_suivi_intervention_search_view</field>        <field name="model">is.suivi.intervention</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="affaire_id"/>                <field name="client_id"/>                <field name="intitule"/>                <field name="article_id"/>                <field name="associe_id"/>                <field name="sous_traitant_id"/>                <group string="Regroupement par">                    <filter string="Affaire"       context="{'group_by': 'affaire_id'}"/>                    <filter string="Client"        context="{'group_by': 'client_id'}"/>                    <filter string="AssociÃ©"       context="{'group_by': 'associe_id'}"/>                    <filter string="Sous-Traitant" context="{'group_by': 'sous_traitant_id'}"/>                    <filter string="Mois"          context="{'group_by': 'date:month'}"/>                </group>            </search>        </field>    </record>    <record id="is_suivi_intervention_graph" model="ir.ui.view">         <field name="name">is_suivi_intervention_graph</field>         <field name="model">is.suivi.intervention</field>         <field name="arch" type="xml">             <graph type="pivot">                 <field name="affaire_id" type="row"/>                 <field name="associe_id" type="col"/>                 <field name="montant_facture"  type="measure"/>             </graph>         </field>    </record>    <record id="is_suivi_intervention_action" model="ir.actions.act_window">        <field name="name">Suivi des interventions</field>        <field name="res_model">is.suivi.intervention</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form,graph</field>    </record></data></openerp>